GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/HexColors" "${PODS_ROOT}/Headers/Public/Masonry"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/HexColors" "$CONFIGURATION_BUILD_DIR/Masonry"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HexColors" -isystem "${PODS_ROOT}/Headers/Public/Masonry"
OTHER_LDFLAGS = $(inherited) -ObjC -l"HexColors" -l"Masonry" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods
